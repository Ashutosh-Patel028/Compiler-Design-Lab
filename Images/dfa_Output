digraph {
	rankdir=LR
	node [shape=circle]
	q0
	node [shape=none]
	""
	"" -> q0 [arrowsize=2 color=red]
	q0 -> q0 [label=a]
	node [shape=circle]
	q0q1
	q0 -> q0q1 [label=b]
	q0q1 -> q0 [label=a]
	node [color=blue shape=doublecircle]
	q0q1q2
	q0q1 -> q0q1q2 [label=b]
	q0q1q2 -> q0 [label=a]
	q0q1q2 -> q0q1q2 [label=b]
	label="\n\nDFA"
	fontsize=20
}
