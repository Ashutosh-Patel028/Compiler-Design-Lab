digraph {
	rankdir=LR
	node [shape=circle]
	Q1
	node [shape=circle]
	Q2
	node [color=blue shape=doublecircle]
	Q3
	node [color=blue shape=doublecircle]
	Q4
	node [color=blue shape=doublecircle]
	Q5
	node [shape=none]
	""
	"" -> Q1 [arrowsize=2 color=red]
	Q1 -> Q2 [label=a]
	Q2 -> Q4 [label=b]
	Q2 -> Q3 [label=c]
	Q2 -> Q5 [label=d]
	label="\nREGEX = ac+ab+ad"
	fontsize=20
}
